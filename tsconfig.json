{
    "compilerOptions": {
        "target": "es6",
        "module": "none",
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noEmitOnError": true,
        "inlineSourceMap": true,
        "inlineSources": true
    }
}
